include ../mixins/_mixin
doctype html
html
	head
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1.0, shrink-to-fit=no')
		title TITLE
		link(media='all', rel='stylesheet', href='css/main.css')
		script(type='text/javascript', src='http://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js', defer)
		script(type='text/javascript').
			window.jQuery || document.write('<script src="js/jquery-3.3.1.min.js" defer><\/script>')
		script(type='text/javascript', src='js/jquery.main.js', defer)
	//- -var bodyClass= "home"; // add class on body
	body(class= bodyClass)
		#wrapper
			block content
